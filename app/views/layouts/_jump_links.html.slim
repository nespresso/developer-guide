.navbar-inverse
  = semantic_navigation :jump_links do |navigation|
    - navigation.item t('.home_page'), root_path, id: 'jump_to_home_page', link_html: {accesskey: 0} unless current_page?(root_path)
    - navigation.item t('.navigation'), '#logo', id: 'jump_to_navigation', link_html: {accesskey: 1}
    - navigation.item t('.content'), '#headline', id: 'jump_to_content', link_html: {accesskey: 2}
